{
  "blux10k": {
    "metadata": {
      "name": "BLUX10K",
      "version": "1.0.0",
      "description": "Professional Developer Terminal Setup for all platforms",
      "repository": "https://github.com/Justadudeinspace/blux10k",
      "author": {
        "name": "Justadudeinspace",
        "github": "https://github.com/Justadudeinspace",
        "ecosystem": "https://github.com/Outer-Void"
      },
      "license": "MIT",
      "created": "2024-01-01",
      "last_updated": "2024-01-01",
      "status": "active"
    },
    "specifications": {
      "shell": {
        "primary": "zsh",
        "minimum_version": "5.0.8",
        "compatible_shells": ["zsh", "bash"],
        "default_shell_set": true
      },
      "platforms": {
        "linux": {
          "distributions": ["Debian", "Ubuntu", "Arch", "Fedora", "openSUSE", "Alpine", "Gentoo"],
          "package_managers": ["apt", "pacman", "dnf", "zypper", "apk", "emerge"]
        },
        "bsd": {
          "distributions": ["FreeBSD", "NetBSD", "OpenBSD"],
          "package_managers": ["pkg", "ports"]
        },
        "macos": {
          "package_managers": ["Homebrew", "MacPorts"],
          "requirements": ["Xcode Command Line Tools"]
        },
        "windows": {
          "environments": ["WSL2", "MSYS2", "Git Bash"],
          "package_managers": ["apt", "pacman"]
        },
        "android": {
          "environments": ["Termux", "Termux-Proot"],
          "package_managers": ["pkg"]
        }
      },
      "architecture": {
        "supported": ["x86_64", "aarch64", "armv7l"],
        "primary": "x86_64"
      }
    },
    "core_components": {
      "plugin_managers": {
        "primary": "zplug",
        "secondary": ["zinit", "zgenom", "znap"],
        "configuration": {
          "zplug": {
            "home": "~/.zplug",
            "cache_enabled": true,
            "threads": 16
          }
        }
      },
      "theme_system": {
        "primary": "powerlevel10k",
        "alternatives": ["starship", "pure"],
        "configuration_files": [".p10k.zsh", ".config/starship.toml"]
      },
      "configuration_management": {
        "primary_config": "~/.zshrc",
        "modular_configs": [
          "~/.config/private/env.zsh",
          "~/.zsh_aliases",
          "~/.zshrc.local"
        ],
        "backup_system": true,
        "auto_compile": true
      }
    },
    "feature_set": {
      "performance": {
        "instant_prompt": true,
        "async_loading": true,
        "compiled_configs": true,
        "lazy_plugins": true
      },
      "development_tools": {
        "modern_replacements": {
          "ls": "eza",
          "cat": "bat",
          "find": "fd",
          "grep": "ripgrep",
          "cd": "zoxide"
        },
        "fuzzy_finder": "fzf",
        "package_managers": ["npm", "pip", "cargo", "gem", "composer"],
        "version_managers": ["volta", "rustup", "pyenv"]
      },
      "productivity": {
        "syntax_highlighting": true,
        "auto_suggestions": true,
        "history_management": {
          "size": 100000,
          "file_rotation": "monthly",
          "advanced_search": true
        },
        "git_integration": {
          "aliases": true,
          "prompt_status": true,
          "auto_completion": true
        }
      },
      "system_management": {
        "update_function": {
          "version": "2.1.0",
          "package_managers": ["apt", "brew", "flatpak", "snap", "pip", "npm", "cargo"],
          "features": ["dry_run", "parallel", "retry_logic", "logging"]
        },
        "health_checks": true,
        "cross_platform": true
      }
    },
    "plugin_ecosystem": {
      "essential_plugins": [
        {
          "name": "zsh-syntax-highlighting",
          "purpose": "Command syntax highlighting",
          "load_order": "defer:2"
        },
        {
          "name": "zsh-autosuggestions",
          "purpose": "Command suggestions from history",
          "config": {
            "strategy": ["history", "completion"],
            "async": true
          }
        },
        {
          "name": "zsh-history-substring-search",
          "purpose": "Enhanced history search",
          "keybindings": ["^[[A", "^[[B"]
        },
        {
          "name": "zsh-completions",
          "purpose": "Additional completion definitions"
        },
        {
          "name": "zsh-z",
          "purpose": "Directory jumping",
          "config": {
            "command": "z",
            "case_sensitivity": "smart"
          }
        }
      ],
      "productivity_plugins": [
        {
          "name": "zsh-autoswitch-virtualenv",
          "purpose": "Auto Python virtualenv activation"
        },
        {
          "name": "fzf-tab",
          "purpose": "Enhanced tab completion"
        },
        {
          "name": "zsh-autopair",
          "purpose": "Auto-pair brackets and quotes"
        }
      ],
      "oh_my_zsh_plugins": [
        "git",
        "command-not-found",
        "colored-man-pages",
        "sudo"
      ]
    },
    "dependency_tree": {
      "critical": {
        "shell": [
          {
            "name": "zsh",
            "minimum_version": "4.3.9",
            "preferred_version": "5.0.8"
          },
          {
            "name": "git",
            "minimum_version": "1.7",
            "preferred_version": "2.4.11"
          }
        ],
        "system_tools": [
          "curl", "wget", "gnupg2", "openssh-client", "ca-certificates"
        ],
        "compression": [
          "unzip", "zip", "tar", "gzip", "bzip2", "xz-utils", "p7zip-full"
        ],
        "network": [
          "lsof", "iproute2", "net-tools"
        ]
      },
      "recommended": {
        "modern_tools": [
          "neovim", "fzf", "fd", "ripgrep", "bat", "eza", "zoxide"
        ],
        "system_info": [
          "neofetch", "fastfetch"
        ],
        "language_toolchains": [
          "python3", "nodejs", "rust", "golang", "ruby"
        ]
      },
      "optional": {
        "containers": ["docker", "podman"],
        "package_managers": ["snapd", "flatpak"],
        "firmware": ["fwupd"]
      }
    },
    "configuration_files": {
      "core": {
        "zshrc": {
          "path": "~/.zshrc",
          "sections": 24,
          "features": [
            "universal_update_function",
            "cross_platform_paths",
            "plugin_system",
            "performance_optimizations"
          ]
        }
      },
      "themes": {
        "powerlevel10k": {
          "path": "~/.p10k.zsh",
          "variants": ["lean", "pure", "rainbow"],
          "customization": "extensive"
        },
        "starship": {
          "path": "~/.config/starship.toml",
          "features": ["cross_shell", "fast", "modular"]
        }
      },
      "applications": {
        "neofetch": {
          "path": "~/.config/neofetch/config.conf",
          "blux10k_theme": true
        }
      },
      "private": {
        "environment": {
          "path": "~/.config/private/env.zsh",
          "permissions": "0600",
          "template": true
        }
      }
    },
    "installation": {
      "methods": {
        "automated": {
          "script": "install.sh",
          "features": [
            "platform_detection",
            "dependency_checking",
            "modular_installation",
            "post_install_setup"
          ]
        },
        "manual": {
          "steps": 10,
          "documentation": "docs/INSTALLATION.md"
        }
      },
      "platform_specific": {
        "debian_ubuntu": "scripts/install-ubuntu-debian.sh",
        "arch": "scripts/install-arch.sh",
        "fedora": "scripts/install-fedora.sh",
        "macos": "scripts/install-macos.sh",
        "termux": "scripts/install-termux.sh",
        "windows": "scripts/install-windows.sh"
      }
    },
    "user_interface": {
      "prompt_systems": {
        "primary": "powerlevel10k",
        "configuration": {
          "segments": [
            "context", "dir", "vcs", "virtualenv", "command_execution_time"
          ],
          "features": [
            "instant_prompt",
            "transient_prompt",
            "icons",
            "colors"
          ]
        }
      },
      "color_scheme": {
        "name": "BLUX10K Professional",
        "primary": "blue",
        "accent": "cyan",
        "success": "green",
        "warning": "yellow",
        "error": "red",
        "background": "transparent"
      },
      "fonts": {
        "required": "MesloLGS NF",
        "variants": [
          "Regular", "Bold", "Italic", "Bold Italic"
        ],
        "alternatives": [
          "ProFontIIxNerdFont",
          "ProFontWindowsNerdFont"
        ]
      }
    },
    "update_management": {
      "update_function": {
        "version": "2.1.0",
        "scopes": {
          "system": ["apt", "snap", "flatpak", "brew", "nix", "firmware"],
          "dev": ["docker", "podman", "conda", "mamba", "asdf", "sdkman"],
          "lang": ["pip", "pipx", "npm", "yarn", "pnpm", "cargo", "rust", "gem", "composer"],
          "frameworks": ["ohmyzsh", "antigen", "zinit"]
        },
        "features": {
          "dry_run": true,
          "parallel_operations": true,
          "retry_with_backoff": true,
          "colored_output": true,
          "logging_system": true,
          "cleanup_options": true
        },
        "environment": {
          "SYSUPDATE_LOG_KEEP": 10,
          "SYSUPDATE_KERNELS_KEEP": 2,
          "SYSUPDATE_MAX_RETRIES": 3,
          "SYSUPDATE_TIMEOUT": 300,
          "SYSUPDATE_SKIP_PIP": 1
        }
      }
    },
    "documentation": {
      "guides": {
        "installation": "docs/INSTALLATION.md",
        "configuration": "docs/CONFIGURATION.md",
        "troubleshooting": "docs/TROUBLESHOOTING.md",
        "platforms": "docs/PLATFORMS.md",
        "customization": "docs/CUSTOMIZATION.md"
      },
      "references": {
        "command_reference": "b10k --help",
        "plugin_reference": "docs/PLUGINS.md",
        "theme_reference": "docs/THEMES.md"
      },
      "community": {
        "github": "https://github.com/Justadudeinspace/blux10k",
        "ecosystem": "https://github.com/Outer-Void",
        "issues": "https://github.com/Justadudeinspace/blux10k/issues"
      }
    },
    "security": {
      "considerations": {
        "private_data": {
          "location": "~/.config/private/",
          "permissions": "0600",
          "git_exclusion": true
        },
        "ssh_keys": {
          "recommended_type": "ed25519",
          "agent_management": true
        },
        "environment_variables": {
          "secure_handling": true,
          "validation": true
        }
      }
    },
    "performance": {
      "optimizations": {
        "zsh_compilation": true,
        "cache_system": true,
        "lazy_loading": true,
        "async_operations": true
      },
      "metrics": {
        "startup_time": "<100ms",
        "memory_usage": "<50MB",
        "plugin_load_time": "optimized"
      }
    },
    "compatibility": {
      "shells": {
        "primary": "zsh",
        "secondary": "bash",
        "tested_versions": ["5.0.8", "5.8", "5.9"]
      },
      "terminals": {
        "recommended": ["Alacritty", "Kitty", "WezTerm", "iTerm2", "Windows Terminal"],
        "tested": ["GNOME Terminal", "Konsole", "Terminator", "Termux"]
      },
      "operating_systems": {
        "primary": "Linux",
        "fully_supported": ["Ubuntu", "Debian", "Arch", "Fedora", "macOS"],
        "community_supported": ["Windows WSL2", "Termux", "BSD variants"]
      }
    },
    "development": {
      "structure": {
        "modular": true,
        "configurable": true,
        "extensible": true,
        "maintainable": true
      },
      "testing": {
        "automated": true,
        "platform_coverage": ["ubuntu", "arch", "macos"],
        "test_files": ["tests/test-installation.sh", "tests/test-configs.sh"]
      },
      "ci_cd": {
        "github_actions": true,
        "test_matrix": ["ubuntu-latest", "macos-latest"],
        "release_automation": true
      }
    },
    "roadmap": {
      "v1.1.0": [
        "Enhanced plugin management",
        "More theme variants",
        "Improved Windows support"
      ],
      "v1.2.0": [
        "Web dashboard",
        "Configuration GUI",
        "Plugin marketplace"
      ],
      "future": [
        "Machine learning enhancements",
        "Cloud synchronization",
        "Mobile terminal support"
      ]
    },
    "statistics": {
      "lines_of_configuration": 2000,
      "plugin_count": 15,
      "alias_count": 50,
      "function_count": 20,
      "supported_platforms": 6,
      "documentation_pages": 5
    }
  }
}